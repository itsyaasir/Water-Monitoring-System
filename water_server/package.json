{
  "name": "water_server",
  "version": "1.0.0",
  "description": "Server",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon --exec babel-node ./server.js",
    "test": "nyc --reporter=text mocha --config=test/.mocharc.json",
    "test:watch": "mocha --config=test/.mocharc.json -- --watch",
    "sequelize": "babel-node node_modules/.bin/sequelize $*",
    "db:drop": "dropdb --if-exists ${POSTGRES_DB}",
    "db:create": "createdb ${POSTGRES_DB}",
    "db:migrate": "yarn sequelize db:migrate",
    "db:rollback": "yarn sequelize db:migrate:undo"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.20.12",
    "@babel/node": "^7.20.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/register": "^7.18.9",
    "@babel/runtime": "^7.20.13",
    "axios": "^1.3.3",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "chalk": "4.1.2",
    "cls-rtracer": "^2.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "eslint-config-airbnb-base": "^15.0.0",
    "express": "^4.18.2",
    "express-joi-validation": "^5.0.1",
    "express-validation": "^4.1.0",
    "http": "0.0.1-security",
    "joi": "^17.7.1",
    "joi-extract-type": "^15.0.8",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.20",
    "path": "^0.12.7",
    "pg": "^8.9.0",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.28.0",
    "sequelize-cli": "^6.6.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "babel-jest": "^29.4.3",
    "babel-loader": "^9.1.2",
    "eslint": "^8.34.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest": "^29.4.3",
    "lint-staged": "^13.1.2",
    "supertest": "^6.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "yarn lint",
      "git add"
    ]
  }
}
